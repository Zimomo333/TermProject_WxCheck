<!--pages/home/scan/scan.wxml-->
<form catchsubmit="formSubmit" catchreset="formReset">
  <view class="cu-form-group margin-top">
    <view class="title">头像</view>
    <image bindtap="bindViewTap" class="userinfo-avatar" src="{{photo}}" mode="cover"></image>
  </view>
  <view class="cu-form-group">
    <view class="title">住户id</view>
    {{id}}
  </view>
  <view class="cu-form-group">
    <view class="title">姓名</view>
    {{name}}
  </view>
  <view class="cu-form-group">
    <view class="title">性别</view>
    {{sex}}
  </view>
  <view class="cu-form-group">
    <view class="title">住址号</view>
    {{house_no}}
  </view>
  <view class="cu-form-group">
    <view class="title">连续高烧天数</view>
    {{health_status}}
  </view>
  <view class="cu-form-group">
    <view class="title">出入次数</view>
    {{access_times}}
  </view>
  <view class="cu-form-group">
    <view class="title">锁定状态</view>
    {{is_locked}}
  </view>
  <view class="cu-form-group">
    <view class="title">体温</view>
    <input name="temperature" name="temperature" value="{{form.temperature}}" placeholder="请输入体温"></input>
  </view>
  <view class="btn-area">
    <button style="margin-top: 10px;margin-bottom:10px;" type="primary" formType="submit">提交记录</button>
  </view>
</form>


